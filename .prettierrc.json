{
    "experimentalTernaries": true,
    "printWidth": 80,
    "tabWidth": 4,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "jsxSingleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "bracketSameLine": false,
    "objectWrap": "collapse",
    "arrowParens": "always",
    "proseWrap": "always",
    "endOfLine": "lf",
    "singleAttributePerLine": false
}
