{
    "[css][javascript][typescript][javascriptreact][typescriptreact][markdown]": {
        "coc.preferences.formatOnSave": true
    },
    "python.linting.pylintEnabled": true,
    "python.pythonPath": "/usr/bin/python",
    "clangd.path": "~/.config/coc/extensions/coc-clangd-data/install/12.0.1/clangd_12.0.1/bin/clangd",
    "[c][cpp]": {
        "semanticTokens.enable": true
    },
    "html.autoClosingTags": false,
    "typescript.autoClosingTags": false,
    "javascript.autoClosingTags": false,
    "suggest.noselect": true,
    "pyright.inlayHints.functionReturnTypes": false,
    "pyright.inlayHints.variableTypes": false,
    "pyright.inlayHints.parameterTypes": false,
    "git.addedSign.hlGroup": "GitSignsAdd",
    "git.changedSign.hlGroup": "GitSignsChange",
    "git.removedSign.hlGroup": "GitSignsDelete",
    "git.topRemovedSign.hlGroup": "GitSignsDelete",
    "git.changeRemovedSign.hlGroup": "GitSingsChange",
    "Lua.workspace.library": ["/usr/share/nvim/runtime"],
    "eslint.autoFixOnSave": true,
    "markdown-preview-enhanced.previewTheme": "github-light.css",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    }
}
