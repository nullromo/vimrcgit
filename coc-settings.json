{
  "[css][javascript][typescript][javascriptreact][typescriptreact][markdown]": {
    "coc.preferences.formatOnSave": true
  },
  "python.linting.pylintEnabled": true,
  "python.pythonPath": "/usr/bin/python",
  "clangd.path": "~/.config/coc/extensions/coc-clangd-data/install/12.0.1/clangd_12.0.1/bin/clangd",
  "[c][cpp]": {
    "semanticTokens.enable": true
  },
  "html.autoClosingTags": false,
  "typescript.autoClosingTags": false,
  "javascript.autoClosingTags": false,
  "suggest.noselect": true,
  "pyright.inlayHints.functionReturnTypes": false,
  "pyright.inlayHints.variableTypes": false,
  "sumneko-lua.enableNvimLuaDev": true,
  "Lua.diagnostics.globals": ["vim"],
  "languageserver": {
    "lua": {
      "command": "/opt/lua-language-server-3.6.25-linux-x64/lua-language-server.sh",
      "args": ["-E", "/opt/lua-language-server-3.6.25-linux-x64/bin/main.lua"],
      "filetypes": ["lua"],
      "settings": {
        "Lua": {
          "workspace.library": {
            "/usr/share/nvim/runtime/lua": true,
            "/usr/share/nvim/runtime/lua/vim": true,
            "/usr/share/nvim/runtime/lua/vim/lsp": true
          },
          "diagnostics": {
            "globals": ["vim"]
          }
        }
      }
    }
  }
}
